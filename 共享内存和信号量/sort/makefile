all:	bubblesort bubblesort1 selectsort selectsort1 insertsort shellsort quicksort mergesort mergesort1\
	heapsort countsort countsort1 countsort2 bucketsort radixsort 

bubblesort:bubblesort.c
	gcc -g -o bubblesort bubblesort.c

bubblesort1:bubblesort1.c
	gcc -g -o bubblesort1 bubblesort1.c

selectsort:selectsort.c
	gcc -g -o selectsort selectsort.c

selectsort1:selectsort1.c
	gcc -g -o selectsort1 selectsort1.c

insertsort:insertsort.c
	gcc -g -o insertsort insertsort.c

shellsort:shellsort.c
	gcc -g -o shellsort shellsort.c

quicksort:quicksort.c
	gcc -g -o quicksort quicksort.c

mergesort:mergesort.c
	gcc -g -o mergesort mergesort.c

mergesort1:mergesort1.c
	gcc -g -o mergesort1 mergesort1.c

heapsort:heapsort.c
	gcc -g -o heapsort heapsort.c

countsort:countsort.c
	gcc -g -o countsort countsort.c

countsort1:countsort1.c
	gcc -g -o countsort1 countsort1.c

countsort2:countsort2.c
	gcc -g -o countsort2 countsort2.c

bucketsort:bucketsort.c
	gcc -g -o bucketsort bucketsort.c

radixsort:radixsort.c
	gcc -g -o radixsort radixsort.c

clean:
	rm -rf bubblesort bubblesort1 selectsort selectsort1 insertsort shellsort quicksort mergesort mergesort1
	rm -rf heapsort countsort countsort1 countsort2 bucketsort radixsort
